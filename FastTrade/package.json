    "pako": "^1.0.6",
    "pure-render-decorator": "^1.1.1",
    "react": "^15.0.1",
    "react-color": "^2.10.0",
    "react-datagrid": "^2.1.1",
    "react-datepicker": "^0.43.0",
    "react-dom": "^15.0.1",
    "react-fileupload": "^2.4.0",
    "superagent": "^2.0.0",
    "winston": "^2.3.1"
  },
