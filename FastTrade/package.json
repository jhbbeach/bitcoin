{
  "name": "option",
  "version": "1.0.4",
  "description": "option by quantdo",
  "main": "main.js",
  "author": "Quantdo",
  "scripts": {
    "starttemp": "webpack -d --watch --progress",
    "start": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js -d --watch --progress",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "creatDll": "webpack --config dll.config.js",
    "publish-mac": "export NODE_ENV=prod&&webpack -p --progress --colors",
    "publish-win": "set NODE_ENV=prod&&webpack -p --progress --colors",
    "packager-asar": "asar pack ./ ../app.asar",
    "build-win": "electron-packager ./ Option --platform=win32 --arch=all --electronVersion=1.6.1 --out ../OutApp --version 1.0.0 --overwrite --icon=./pages-react/img/log256.ico --ignore=node_modules/extract-* --ignore=node_modules/electron-* --ignore=node_modules/webpack-* --ignore=node_modules/webpack --ignore=node_modules/babel-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune",
    "build-osx": "electron-packager ./ Option --platform=darwin --arch=all --electronVersion=1.7.9 --out ../OutApp --version 1.0.0 --overwrite --icon=./pages-react/img/electron.icns --ignore=node_modules/extract-* --ignore=node_modules/electron-* --ignore=node_modules/webpack-* --ignore=node_modules/webpack --ignore=node_modules/babel-* --ignore=node_modules/.bin --ignore=.git --ignore=dist --prune",
    "build-linux": "electron-packager ./ Option --platform=linux --arch=all --asar --electronVersion=1.6.1 --out ../OutApp --version 1.0.0 --overwrite --icon=./pages-react/img/electron.icns"
  },
  "dependencies": {
    "config": "^1.28.1",
    "getmac": "^1.2.1",
